<UserControl x:Class="EnduranceJudge.Gateways.Desktop.Views.Content.PrintExample.PrintExampleView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True">

    <StackPanel Name="PrintRoot">
        <TextBlock>Hello, I am printed</TextBlock>
        <Button Command="{Binding Print}" CommandParameter="{Binding ElementName=PrintRoot}">Print</Button>
    </StackPanel>

</UserControl>
